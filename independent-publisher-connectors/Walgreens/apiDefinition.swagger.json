{
  "swagger": "2.0",
  "info": {
    "title": "Walgreens",
    "description": "The Walgreens Developer Program allows you access to tap into the large and growing number of Walgreens stores, products, and more!",
    "version": "1.0",
    "contact": {
      "name": "Troy Taylor",
      "url": "https://www.hitachisolutions.com",
